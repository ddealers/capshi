*{
	margin: 0;
	padding: 0;
}
html{
	height: 100%;
	width: 100%;
	body {
		height: 100%;
		width: 100%;
        font-family: "PT Sans", Helvetica, Arial, sans-serif;  
		.part-1{
			position: relative;
			width: 100%;
			height: 100%;
			background: url("/img/Family_Transparent1.png") center;
            background-repeat: no-repeat;
            background-size: cover;
			.navigation {
				position: fixed;
				width: 100%;
				list-style: none;
                margin: 0;
                background: url("/img/fondo-menu.png");
                background-size: cover;
                background-repeat: no-repeat;    
                display: -webkit-box;
                display: -moz-box;display: -ms-flexbox;
                display: -webkit-flex;
                display: flex;
				-webkit-flex-flow: row wrap;
				justify-content: flex-end;
				z-index: 2;
			}
			.navigation a {
  				text-decoration: none;
  				display: block;
  				padding: 1em;
  				color: white;
  				font-family: arial;
  				text-align: center; 
    			//padding: 15px;
			}
			.navigation a:hover {
  				color: darken(#448C42, 2%);
                text-shadow: 4px 4px 4px rgba(0, 0, 0, 0.3);                  
			}
			.container-part-1{
				justify-content: center;
				width: 100%;
				position: relative;
				top: 90px;
  				height: auto;
  				#tex-1{
  					font-family: arial;
  					font-size: 40px;
  					text-align: center;
  				}
  				#tex-2{
  					width: 100%;
  					position: relative;
  					text-align: center;
  					font-size: 22px;			
  				}
  			}
		}
		.part-2{
			position: relative;	
			width: 100%;
			height: 100%;
            background: url("/img/manos-fam.png") center fixed;
            background-repeat: no-repeat;
            background-size: cover;
            display: flex;
			.quienes-somos{	
                display: flex;         
                position: relative;
                width: 550px;
                height: 550px;
                border-radius: 50%;
                background: rgba(0,0,0,0.69);
                padding: 10px;
                top: 70px;    
                margin: auto;    
				#logo{
					position: absolute;
                    left: 36%;
                    width: 160px;
                    height: 160px;
                    padding: 0;
                    top: 60px;
                    border-radius: 50%;
                    border: solid 3px #fff              
				}
				#tex-3{
                    position: relative;
                    top: 50px;
                    width: 80%;
                    text-align: justify;
                    font-size: 18px;
                    color: #fff;
                    margin: auto;
				}
            }
        }
        .part-3{
            position: relative;
            width: 100%;
            height: 100%;
            background: url("/img/fondo2.jpg") center fixed;
            background-size: cover;
            background-repeat: no-repeat;
            display: flex;
            .que-ofrecemos{
                position: relative;
                width: 550px;
                height: 550px;
                border-radius: 50%;
                background: rgba(0,0,0,0.69);
                padding: 10px;
                top: 30px;  
                margin: auto;
                #tex-4{
                    position: relative;
                    top: 110px;
                    width: 80%;
                    text-align: justify;
                    padding: 51px;
                    font-size: 18px;
                    color: white;
                }
            }
        }
        .servicios{
            position: relative;
            width: 100%;
            height: 100%;
            display: flex;
            h1 {
                position: absolute;
                text-align: center;
                width: 100%;
                font-size: 45px;
                top: 60px;
            }
            #des-humano{
                position: absolute;
                width: 30%;
                height:10px;
                top: 130px;
                font-size: 17px;
                padding: 16px;
            }           
            .flex-container-des-humano {
                padding: 0;
                margin: 0;
                list-style: none;
                display: -webkit-box;
                display: -moz-box;
                display: -ms-flexbox;
                display: -webkit-flex;
                display: flex;
                -webkit-flex-flow: row wrap;
                justify-content: center;
                width: 100%;
                position: absolute;
                top: 180px;
            }
            .flex-item-ter-alte {
                width: 200px;
                height: 150px;
            }
            .icono-terapia{
                width: 100px;  
                img {
                    width: 50%;
                }   
            }
            #te-alternas{
                position: absolute;
                width: 30%;
                top: 300px;
                font-size: 17px;
                padding: 16px;
            }
            .flex-container-ter-alter{
                padding: 0;
                margin: 0;
                list-style: none;
                display: -webkit-box;
                display: -moz-box;
                display: -ms-flexbox;
                display: -webkit-flex;
                -webkit-flex-flow: row wrap;
                justify-content: center;
                width: 100%;
                position: absolute;
                top: 360px;
                height: 300px;
                img {
                    width: 50%;
                }
            }
            .fondo-rgba{
                width: 100%;
                height: 100%;
                position: fixed;
                background: rgba(0,0,0,0.8);
                z-index: 1;
                top: 0;
                left: 0;
                display: none;
                .info-terapias{
                    width: 550px;
                    height: 550px;
                    border-radius: 50%;
                    z-index: 2;
                    background: url("/img/fondo.contacto1.png") no-repeat center;
                    background-size: cover;
                    top: 100px;
                    margin: auto;
                    position: relative;
                    .im-cierre{
                        position: absolute;
                        width: 100px;
                        left: 250px;
                        top: 15px;
                        img {
                            width: 60%;
                        }
                    }     
                    .titulo-terapia{
                        position: relative;
                        width: 70%;
                        font-size: 34px;
                        top: 87px;
                        margin: auto;
                    }
                    .cont-terapia{
                        position: absolute;
                        width: 80%;
                        height: 200px;
                        text-align: justify;
                        top: 200px;
                        left: 56px; 
                        overflow: scroll;
                    }    
                }
            }           
        }
        .contacto{
            position: relative;
            width: 100%;
            height: 100%;
            display: flex;
            margin: auto;
            background: url("/img/fondo.menu1.png");
            background-size: cover;
            #tit-contacto {
                position: absolute;
                width: 100%;
                text-align: center;
                top: 100px;
                font-size: 40px;
            }
            #tex-contacto{
                position: absolute;
                width: 100%;
                height: 10px;
                font-size: 17px;
                text-align: center;
                top: 160px;
            }
            .formulario{
                position: relative;
                width: 46%;
                height: 380px;
                display: flex;
                top: 250px;
                /* margin: auto; */
                left: 30%;
                #name-form{
                    position: relative;
                    width: 100px;
                    height: 30px;
                    font-size: 20px;
                    left: 20px;    
                }
                #name-text{
                    position: absolute;
                    width: 50%;
                    height: 25px;
                    border-radius: 10px;
                    top: 30px;
                    border: none;
                    border: 2px solid #4ba55b;
                    font-size: 18px;
                    padding: 5px;
                    margin: auto;
                    color: #4ba55b;
                }
                #mail-form{
                    position: absolute;
                    width: 100px;
                    height: 30px;
                    font-size: 20px;
                    top: 88px;
                    left: 20px;
                    }
                #mail-text{
                    position: absolute;
                    width: 50%;
                    height: 25px;
                    border-radius: 10px;
                    top: 120px;
                    margin: auto;;
                    border: none;
                    border: 2px solid #4BA55B;
                    font-size: 18px;
                    padding: 5px;
                    color: #4ba55b;
                }
                #boton-enviar{
                    position: relative;
                    width: 100px;
                    background-color: #4ba55b;
                    height: 20px;
                    border-radius: 10px;
                    top: 60px;
                    right: -18px;
                    text-decoration: none;
                    color: #fff;
                    text-align: center;
                    margin: auto;
                    padding: 9px;
                    right: 13%;
                }
                #telefono {
                    width: 150px;
                    margin: auto;
                    top: 320px;
                    right: 269px;
                    cursor: pointer;
                    margin: auto;
                    position: absolute;
                    img {
                        width: 20%;
                        mouse: pointer;
                    }
                    #num {
                        font-size: 21px;
                        text-align: center;
                        width: 141px;
                        position: relative;
                        right: 63px;
                        top: 17px;
                        display: none;
                    }
                }    
                #face{
                    position: absolute;
                    right: 184px;
                    top: 320px;
                    width: 150px;
                    img {
                        width: 10%;
                    }
                }
                #ubicacion{
                    width: 25px;
                    height: 34px;
                    top: 318px;
                    position: absolute;
                    right: 235px;
                }
                .form-select {
                    -moz-user-select: -moz-none;
                    -ms-user-select: none;
                    -webkit-user-select: none;
                    user-select: none;
                    background: #ffffff;
                    overflow: hidden;
                    border: 2px solid #4ba55b;
                    border-radius: 10px;
                    -moz-transition: box-shadow 0.2s;
                    -o-transition: box-shadow 0.2s;
                    -webkit-transition: box-shadow 0.2s;
                    transition: box-shadow 0.2s;
                }
                .form-select:focus, .form-select:hover {
                    box-shadow: 0 0 15px rgba(0, 0, 0, 0.2);
                } 
                .form-select:after {
                    content: '▾';
                    position: absolute;
                    top: 0;
                    right: 12px;
                    z-index: 1;
                    color: #4ba55b;
                    line-height: 40px;
                    font-size: 18px;       
                }
                #lista-terapias{
                    position: relative;
                    background: transparent;
                    background-image: none;
                    display: block;
                    width: 100%;
                    outline: none;
                    color: #4ba55b;
                    cursor: pointer;
                    z-index: 2;
                    border: none;
                    padding: 10px 10px 10px 5px;
                    text-overflow: '';
                    text-indent: 10px;
                    -moz-appearance: none;
                    -webkit-appearance: none;
                    font-size: 15px;
                }
                #lista-terapias[multiple=multiple] {
                    height: auto;
                    border: 1px solid #cbcbcb;
                    border-radius: 3px;
                    box-shadow: 0 0 1px rgba(0, 0, 0, 0.1), inset 0 0 10px rgba(0, 0, 0, 0.07);
                    -moz-transition: box-shadow 0.9s;
                    -o-transition: box-shadow 0.9s;
                    -webkit-transition: box-shadow 0.9s;
                    transition: box-shadow 0.9s;
                }
                #lista-terapias[multiple=multiple]:hover {
                    border-color: #cccccc;
                    box-shadow: 0 0 15px rgba(0, 0, 0, 0.2);
                } 
               #lista-terapias option[disabled] {
                    color: #eeeeee;
                    text-shadow: none;
                    border: none;
                    font-size: 18px;
                }
                #lista-terapias:-moz-focusring {
                    color: transparent;
                    text-shadow: 0 0 0 #888888;
                }
                #lista-terapias::-ms-expand {
                    display: none;
                }
                .column-6 {
                    position: absolute;
                    top: 50%;
                    width: 52%;
                }     
            }   
        }          
    }   
}                